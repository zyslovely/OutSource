<?xml version="1.0" encoding="UTF-8" ?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoogle.teach.mapper.CourseStudentScoreMapper">
	<resultMap type="CourseStudentScore" id="CourseStudentScoreMap ">
		<result property="id" column="id" />
		<result property="courseId" column="courseId" />
		<result property="studentId" column="studentId" />
		<result property="percentType" column="percentType" />
		<result property="percent" column="percent" />
		<result property="score" column="score" />
	</resultMap>
	<insert id="addCourseStudentScore" parameterType="CourseStudentScore"> INSERT INTO
		TB_Course_Student_Score
		(id,courseId,studentId,percentType,percent,score )VALUES(
		#{id},#{courseId},#{studentId},#{percentType},#{percent},#{score})
	</insert>
</mapper>