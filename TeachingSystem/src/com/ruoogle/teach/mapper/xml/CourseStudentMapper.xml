<?xml version="1.0" encoding="UTF-8" ?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoogle.teach.mapper.CourseStudentMapper">
	<resultMap type="CourseStudent" id="courseStudentMap ">
		<result property="id" column="id" />
		<result property="courseId" column="courseId" />
		<result property="semester" column="semester" />
		<result property="classId" column="classId" />
		<result property="studentId" column="studentId" />
	</resultMap>
	<insert id="addCourseStudent" parameterType="CourseStudent">
		INSERT INTO
		TB_Course_Student (
		id,courseId,semester,classId,studentId
		)VALUES(
		#{id},#{courseId},#{semester},#{classId},#{studentId}
		)
	</insert>

	<select id="getCourseStudentsByCourseId" resultMap="CourseStudent">
		SELECT *
		from
		TB_Course_Student
		WHERE courseId=#{courseId};
	</select>



</mapper>